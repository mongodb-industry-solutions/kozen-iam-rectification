{
  "name": "@kozen/iam-rectification",
  "version": "1.0.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "publish": "npm publish --access public",
    "mcp:start": "ts-node bin/kozen.ts --type=mcp",
    "start": "npm run bin:kozen:js --config=kozen.js.json",
    "dev": "npm run bin:kozen:ts --config=kozen.ts.json",
    "build": "tsc && npm run copy:txt",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy:txt": "copyfiles -u 1 src/**/*.txt dist/",
    "bin:kozen:ts": "ts-node node_modules/@kozen/engine/dist/bin/kozen.js",
    "bin:kozen:js": "npx kozen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mongodb-industry-solutions/kozen-iam-rectification.git"
  },
  "keywords": [
    "mongodb",
    "security",
    "kozen",
    "mcp",
    "mcp-server",
    "iam",
    "iam-rectification"
  ],
  "author": "MDB SAT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mongodb-industry-solutions/kozen-iam-rectification/issues"
  },
  "homepage": "https://github.com/mongodb-industry-solutions/kozen-iam-rectification/wiki",
  "description": "Module for the Kozen framework that includes support for running the IAM rectification process",
  "dependencies": {
    "@kozen/engine": "^1.1.13",
    "@mongodb-solution-assurance/iam-util": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
